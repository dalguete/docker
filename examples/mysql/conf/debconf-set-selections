# New password for the MySQL "root" user:
mysql-server-5.5	mysql-server/root_password	password	changeme4urnewpass
mysql-server-5.5	mysql-server/root_password_again	password	changeme4urnewpass
# Web server to reconfigure automatically:
# Choices: apache2, lighttpd
phpmyadmin	phpmyadmin/reconfigure-webserver	multiselect	apache2
# Password of the database's administrative user:
phpmyadmin	phpmyadmin/mysql/admin-pass	password	changeme4urnewpass
# MySQL application password for phpmyadmin:
phpmyadmin	phpmyadmin/mysql/app-pass	password	
# Configure database for phpmyadmin with dbconfig-common?
phpmyadmin	phpmyadmin/dbconfig-install	boolean	true
# Deconfigure database for phpmyadmin with dbconfig-common?
phpmyadmin	phpmyadmin/dbconfig-remove	boolean	true
